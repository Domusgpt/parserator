{
  "name": "@parserator/browser-sdk",
  "version": "1.0.0",
  "description": "Parserator SDK for browser environments",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.types.json",
    "build": "npm run clean && npm run build:tsc",
    "build:bundle": "echo 'Bundling with esbuild/rollup would go here to create UMD etc.'",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "dependencies": {
    "@parserator/core-api-client": "file:../core-api-client",
    "@parserator/types": "file:../types"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "parserator",
    "sdk",
    "browser",
    "frontend",
    "typescript"
  ],
  "author": "Parserator Team",
  "license": "MIT"
}
