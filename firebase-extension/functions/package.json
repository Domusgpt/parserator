  },
  "dependencies": {
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.3.1",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "typescript": "^5.1.6"
  },
  "private": true
}